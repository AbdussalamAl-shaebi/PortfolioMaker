@page "/"
@inject IJSRuntime Js

<InputFile id="fileInput" OnChange="UploadFiles" hidden multiple />
@if (Design)
{
    dsfsfdsdfsdfsdfsdfs
    <MudGrid Spacing="0">
        <MudItem lg="12" xs="12">
            <MudGrid Spacing="0">
                <MudItem  lg="2">
                    <MudButton HtmlTag="label" for="fileInput">
                        @if (ImageDataString != null)
                        {
                            <MudImage Src=@ImageDataString Width="100" Height="100" Alt="Your photo" Elevation="0" Class="rounded-lg ma-4" />
                        }
                        else
                        {
                            <MudImage Src="https://cdn-icons-png.flaticon.com/512/1053/1053244.png" Width="100" Height="100" Alt="Photo" Elevation="0" Class="rounded-lg ma-4" />
                        }
                    </MudButton>
                </MudItem>
                <MudItem lg="10">
                    <MudGrid Spacing="0" Justify="Justify.FlexStart">
                        <MudItem xs="12">
                            <MudItem lg="4">
                                <MudTextField @bind-Value="Name"
                                          Label="Name"
                                          Variant=TextVariant
                                          Margin=TextMargin>
                                </MudTextField>
                            </MudItem>
                        </MudItem>

                        <MudItem xs="12">
                            <MudItem lg="4">
                                <MudTextField @bind-Value="Email"
                                          Label="Email"
                                          Variant=TextVariant
                                          Margin=TextMargin>
                                </MudTextField>
                            </MudItem>
                        </MudItem>
                    </MudGrid>
                </MudItem>
            </MudGrid>
        </MudItem>
        <MudItem xs="12">
            <MudItem lg="6">
                <MudTextField @bind-Value="Phone"
                          Label="Phone"
                          Variant=TextVariant
                          Margin=TextMargin>
                </MudTextField>
            </MudItem>
        </MudItem>
        <MudItem xs="12">
            <MudItem lg="6">
                <MudTextField @bind-Value="LinkedIn"
                          Label="LinkedIn"
                          Variant=TextVariant
                          Margin=TextMargin>
                </MudTextField>
            </MudItem>
        </MudItem>
        <MudItem xs="12">
            <MudTextField @bind-Value="Address"
                      Label="Address"
                      Variant=TextVariant
                      Margin=TextMargin>
            </MudTextField>
        </MudItem>
        <MudItem xs="12" Class="ma-4">

        </MudItem>
        <MudItem xs="12">
            <MudText Typo="Typo.h6">Work Experience</MudText>
        </MudItem>

        <MudItem xs="12">
            <MudItem lg="6">
                <MudTextField @bind-Value="JobTitle1"
                          Label="Last job title"
                          Variant=TextVariant
                          Margin=TextMargin>
                </MudTextField>
            </MudItem>
        </MudItem>
        <MudItem xs="12">
            <MudTextField @bind-Value="JobDescription1"
                      Label="Last job description"
                      Variant=TextVariant
                      Margin=TextMargin>
            </MudTextField>
        </MudItem>

        @if (JobTitleCount >= 2)
        {
            <MudItem xs="12" Class="ma-2">
            </MudItem>

            <MudItem xs="12">
                <MudItem lg="6">
                    <MudTextField @bind-Value="JobTitle2"
                          Label="Other job title"
                          Variant=TextVariant
                          Margin=TextMargin>
                    </MudTextField>
                </MudItem>
            </MudItem>

            <MudItem xs="12">
                <MudTextField @bind-Value="JobDescription2"
                      Label="Other job description"
                      Variant=TextVariant
                      Margin=TextMargin>
                </MudTextField>
            </MudItem>
        }


        @if (JobTitleCount >= 3)
        {
            <MudItem xs="12" Class="ma-2">
            </MudItem>

            <MudItem xs="12">
                <MudItem lg="6">
                    <MudTextField @bind-Value="JobTitle3"
                          Label="Other job title"
                          Variant=TextVariant
                          Margin=TextMargin>
                    </MudTextField>
                </MudItem>
            </MudItem>

            <MudItem xs="12">
                <MudTextField @bind-Value="JobDescription3"
                      Label="Other job description"
                      Variant=TextVariant
                      Margin=TextMargin>
                </MudTextField>
            </MudItem>
        }


        @if (JobTitleCount >= 4)
        {
            <MudItem xs="12" Class="ma-2">
            </MudItem>

            <MudItem xs="12">
                <MudItem lg="6">
                    <MudTextField @bind-Value="JobTitle4"
                          Label="Other job title"
                          Variant=TextVariant
                          Margin=TextMargin>
                    </MudTextField>
                </MudItem>
            </MudItem>

            <MudItem xs="12">
                <MudTextField @bind-Value="JobDescription4"
                      Label="Other job description"
                      Variant=TextVariant
                      Margin=TextMargin>
                </MudTextField>
            </MudItem>
        }



        @if (JobTitleCount < 4)
        {
            <MudItem xs="12" Class="ma-2">
            </MudItem>

            <MudItem xs="12">
                <MudButton OnClick="(()=> JobTitleCount+=1)" Variant="Variant.Outlined" StartIcon="@Icons.Material.Filled.Add" Color="Color.Primary">Add new work experience</MudButton>
            </MudItem>
        }

        <MudItem xs="12" Class="ma-4">
        </MudItem>
        <MudItem xs="12">
            <MudText Typo="Typo.h6">Education</MudText>
        </MudItem>



        <MudItem xs="12">
            <MudItem lg="6">
                <MudTextField @bind-Value="Education1"
                          Label="Last education"
                          Variant=TextVariant
                          Margin=TextMargin>
                </MudTextField>
            </MudItem>
        </MudItem>
        <MudItem xs="12">
            <MudTextField @bind-Value="EducationInstatotion1"
                      Label="Last education school"
                      Variant=TextVariant
                      Margin=TextMargin>
            </MudTextField>
        </MudItem>


        @if (EducationCount >= 2)
        {
            <MudItem xs="12" Class="ma-2">
            </MudItem>

            <MudItem xs="12">
                <MudItem lg="6">
                    <MudTextField @bind-Value="Education2"
                          Label="Other education"
                          Variant=TextVariant
                          Margin=TextMargin>
                    </MudTextField>
                </MudItem>
            </MudItem>

            <MudItem xs="12">
                <MudTextField @bind-Value="EducationInstatotion2"
                      Label="Other education school"
                      Variant=TextVariant
                      Margin=TextMargin>
                </MudTextField>
            </MudItem>
        }


        @if (EducationCount >= 3)
        {
            <MudItem xs="12" Class="ma-2">
            </MudItem>

            <MudItem xs="12">
                <MudItem lg="6">
                    <MudTextField @bind-Value="Education3"
                          Label="Other education"
                          Variant=TextVariant
                          Margin=TextMargin>
                    </MudTextField>
                </MudItem>
            </MudItem>

            <MudItem xs="12">
                <MudTextField @bind-Value="EducationInstatotion3"
                      Label="Other education school"
                      Variant=TextVariant
                      Margin=TextMargin>
                </MudTextField>
            </MudItem>
        }

        @if (EducationCount < 3)
        {
            <MudItem xs="12" Class="ma-2">
            </MudItem>

            <MudItem xs="12">
                <MudButton OnClick="(()=> EducationCount+=1)" Variant="Variant.Outlined" StartIcon="@Icons.Material.Filled.Add" Color="Color.Primary">Add new education</MudButton>
            </MudItem>
        }

        <MudItem xs="12" Class="ma-4">
        </MudItem>
        <MudItem xs="12">
            <MudText Typo="Typo.h6">Languages</MudText>
        </MudItem>

        <MudItem lg="2" xs="6">
            <MudSelect Variant=TextVariant @bind-Value="NativeLanguageName"
                   Margin=TextMargin T="string" Label="Native language" AnchorOrigin="Origin.BottomCenter">
                <MudSelectItem Value="@("Arabic")">Arabic</MudSelectItem>
                <MudSelectItem Value="@("English")">English</MudSelectItem>
            </MudSelect>
        </MudItem>


        <MudItem xs="12" Class="ma-2">
        </MudItem>


        <MudItem lg="2" xs="6">
            <MudSelect T="string" Variant=TextVariant @bind-Value="OtherLanguageName"
                   Margin=TextMargin Label="Other language" AnchorOrigin="Origin.BottomCenter">
                <MudSelectItem Value="@("Arabic")">Arabic</MudSelectItem>
                <MudSelectItem Value="@("English")">English</MudSelectItem>
            </MudSelect>
        </MudItem>

        <MudItem xs="12" Class="ma-2">
        </MudItem>

        <MudItem lg="2" xs="4" Class="mr-8">
            <MudSelect T="string" Variant=TextVariant @bind-Text="SpeakingLevel"
                   Margin=TextMargin Label="Speaking Level" AnchorOrigin="Origin.BottomCenter">
                <MudSelectItem Value="@("A1")">A1</MudSelectItem>
                <MudSelectItem Value="@("A2")">A2</MudSelectItem>
                <MudSelectItem Value="@("B1")">B1</MudSelectItem>
                <MudSelectItem Value="@("B2")">B2</MudSelectItem>
                <MudSelectItem Value="@("C1")">C1</MudSelectItem>
                <MudSelectItem Value="@("C2")">C2</MudSelectItem>
            </MudSelect>
        </MudItem>

        <MudItem lg="2" xs="4" Class="mr-8">
            <MudSelect Variant=TextVariant
                   Margin=TextMargin T="string" @bind-Value ="ListeningLevel" Label="Listening Level" AnchorOrigin="Origin.BottomCenter">
                <MudSelectItem Value="@("A1")">A1</MudSelectItem>
                <MudSelectItem Value="@("A2")">A2</MudSelectItem>
                <MudSelectItem Value="@("B1")">B1</MudSelectItem>
                <MudSelectItem Value="@("B2")">B2</MudSelectItem>
                <MudSelectItem Value="@("C1")">C1</MudSelectItem>
                <MudSelectItem Value="@("C2")">C2</MudSelectItem>
            </MudSelect>
        </MudItem>

        <MudItem lg="2" xs="4" Class="mr-8">
            <MudSelect Variant=TextVariant
                   Margin=TextMargin T="string" @bind-Value ="ReadingLevel" Label="Reading Level" AnchorOrigin="Origin.BottomCenter">
                <MudSelectItem Value="@("A1")">A1</MudSelectItem>
                <MudSelectItem Value="@("A2")">A2</MudSelectItem>
                <MudSelectItem Value="@("B1")">B1</MudSelectItem>
                <MudSelectItem Value="@("B2")">B2</MudSelectItem>
                <MudSelectItem Value="@("C1")">C1</MudSelectItem>
                <MudSelectItem Value="@("C2")">C2</MudSelectItem>
            </MudSelect>
        </MudItem>

        <MudItem lg="2" xs="4" Class="mr-8">
            <MudSelect Variant=TextVariant @bind-Value ="WritingLevel"
                   Margin=TextMargin T="string" Label="Writing Level" AnchorOrigin="Origin.BottomCenter">
                <MudSelectItem Value="@("A1")">A1</MudSelectItem>
                <MudSelectItem Value="@("A2")">A2</MudSelectItem>
                <MudSelectItem Value="@("B1")">B1</MudSelectItem>
                <MudSelectItem Value="@("B2")">B2</MudSelectItem>
                <MudSelectItem Value="@("C1")">C1</MudSelectItem>
                <MudSelectItem Value="@("C2")">C2</MudSelectItem>
            </MudSelect>
        </MudItem>

        <MudItem xs="12" Class="ma-4">
        </MudItem>
        <MudItem xs="12">
            <MudText Typo="Typo.h6">Professional skills & tools</MudText>
        </MudItem>

        <MudItem xs="12">
            <MudGrid Spacing="0">
                <MudItem xs="12" sm="12" lg="3" md="4">
                    <MudTextField @bind-Value="ProfianlSkill"
                              Label="Add professional skills & tools"
                              Variant=TextVariant>
                    </MudTextField>
                </MudItem>
            </MudGrid>
        </MudItem>

        <MudItem xs="12" Class="mt-2">
        </MudItem>

        <MudItem xs="12">
            <MudButtonGroup Color="Color.Primary" Variant="Variant.Outlined">
                <MudIconButton OnClick="()=> {AddToList(ProfianlSkill,ProfianlSkills); ProfianlSkill= string.Empty;}" Icon="@Icons.Material.Filled.Add" Color="Color.Success" aria-label="Add"></MudIconButton>
                <MudIconButton OnClick="()=>{ DeleteFromList(ProfianlSkill,ProfianlSkills);ProfianlSkill= string.Empty;}" Icon="@Icons.Material.Filled.Delete" Color="Color.Error" aria-label="Delete"></MudIconButton>
            </MudButtonGroup>
        </MudItem>

        <MudItem xs="12">
            <MudTextField @bind-Value="AllProfianlSkill"
                      Label=""
                      ReadOnly="true"
                      Variant=TextVariant>
            </MudTextField>
        </MudItem>

        <MudItem xs="12" Class="ma-4">
        </MudItem>
        <MudItem xs="12">
            <MudText Typo="Typo.h6">Other skills & tools</MudText>
        </MudItem>

        <MudItem xs="12">
            <MudGrid Spacing="0">
                <MudItem xs="12" sm="12" lg="3" md="4">
                    <MudTextField @bind-Value="OtherSkill"
                              Label="Add other skills & tools"
                              Variant=TextVariant>
                    </MudTextField>
                </MudItem>
            </MudGrid>
        </MudItem>

        <MudItem xs="12" Class="mt-2">
        </MudItem>

        <MudItem xs="12">
            <MudButtonGroup Color="Color.Primary" Variant="Variant.Outlined">
                <MudIconButton OnClick="()=>{ AddToList(OtherSkill,OtherSkills,false); OtherSkill= string.Empty;}" Icon="@Icons.Material.Filled.Add" Color="Color.Success" aria-label="Add"></MudIconButton>
                <MudIconButton OnClick="()=>{ DeleteFromList(OtherSkill,OtherSkills,false);OtherSkill= string.Empty;}" Icon="@Icons.Material.Filled.Delete" Color="Color.Error" aria-label="Delete"></MudIconButton>
            </MudButtonGroup>
        </MudItem>

        <MudItem xs="12">
            <MudTextField @bind-Value="AllOtherProfianlSkill"
                      Label=""
                      ReadOnly="true"
                      Variant=TextVariant>
            </MudTextField>
        </MudItem>


        <MudItem xs="12" Class="ma-16">
        </MudItem>


    </MudGrid>
}
