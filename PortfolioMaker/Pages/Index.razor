</MudAppBar>







@code
{
    public string Name { get; set; } = string.Empty;
    public string Email { get; set; } = string.Empty;
    public string Address { get; set; } = string.Empty;
    public string Phone { get; set; } = string.Empty;
    public string LinkedIn { get; set; } = string.Empty;

    public string JobTitle1 { get; set; } = string.Empty;
    public string JobTitle2 { get; set; } = string.Empty;
    public string JobTitle3 { get; set; } = string.Empty;
    public string JobTitle4 { get; set; } = string.Empty;
    public string JobTitle5 { get; set; } = string.Empty;

    public string JobDescription1 { get; set; } = string.Empty;
    public string JobDescription2 { get; set; } = string.Empty;
    public string JobDescription3 { get; set; } = string.Empty;
    public string JobDescription4 { get; set; } = string.Empty;
    public string JobDescription5 { get; set; } = string.Empty;


    public string Education1 { get; set; } = string.Empty;
    public string Education2 { get; set; } = string.Empty;
    public string Education3 { get; set; } = string.Empty;

    public string EducationInstatotion1 { get; set; } = string.Empty;
    public string EducationInstatotion2 { get; set; } = string.Empty;
    public string EducationInstatotion3 { get; set; } = string.Empty;


    public int JobTitleCount { get; set; } = 1;
    public int EducationCount { get; set; } = 1;

    public string ProfianlSkill { get; set; } = string.Empty;
    public string AllProfianlSkill { get; set; } = string.Empty;
    public List<string> ProfianlSkills { get; set; } = new();

    public string OtherSkill { get; set; } = string.Empty;
    public string AllOtherProfianlSkill { get; set; } = string.Empty;
    public List<string> OtherSkills { get; set; } = new();
    public bool Design { get; set; } = true;



    public Variant TextVariant { get; set; } = Variant.Text;
    public Margin TextMargin { get; set; } = Margin.Dense;

    public string NativeLanguageName { get; set; } = string.Empty;
    public string OtherLanguageName { get; set; } = string.Empty;

